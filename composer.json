{
    "name": "deligoez/laravel-model-hashids",
    "description": "Generate, save, and route Stripe-like HashIDs for Laravel Eloquent Models",
    "keywords": [
        "deligoez",
        "laravel",
        "laravel-package",
        "laravel-model-hashids",
        "hashids"
    ],
    "homepage": "https://github.com/deligoez/laravel-model-hashids",
    "license": "MIT",
    "type": "library",
    "support": {
        "issues": "https://github.com/deligoez/laravel-model-hashids/issues",
        "source": "https://github.com/deligoez/laravel-model-hashids"
    },
    "authors": [
        {
            "name": "Yunus Emre Delig√∂z",
            "email": "ye@deligoz.me",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "ext-mbstring": "*",
        "hashids/hashids": "^4.1",
        "illuminate/contracts": "^8.57",
        "spatie/laravel-package-tools": "^1.9"
    },
    "require-dev": {
        "brianium/paratest": "^6.3",
        "friendsofphp/php-cs-fixer": "^3.0",
        "nunomaduro/collision": "^5.9",
        "nunomaduro/larastan": "^0.7.12",
        "orchestra/testbench": "^6.20",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^0.12.6",
        "phpstan/phpstan-phpunit": "^0.12.22",
        "phpunit/phpunit": "^9.5",
        "spatie/laravel-ray": "^1.25"
    },
    "autoload": {
        "psr-4": {
            "Deligoez\\LaravelModelHashIDs\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Deligoez\\LaravelModelHashIDs\\Tests\\": "tests"
        }
    },
    "scripts": {
        "lint": "php-cs-fixer fix -v",
        "phpstan": "vendor/bin/phpstan analyse",
        "test": "./vendor/bin/testbench package:test --parallel --no-coverage",
        "test-coverage": "vendor/bin/phpunit --coverage-text"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelModelHashIDsServiceProvider"
            ],
            "aliases": {
                "LaravelModelHashids": "LaravelModelHashIDsFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
